@model ImportExcel.Models.Labor
@{
    ViewData["Title"] = "ShowDB";
}

<h1>ShowDB</h1>
<div>
	<div class="row">
		<div class="col">
			<table>
				@foreach (var l in ViewBag.labors)
				{
					<tr>
						<td>@l.Name</td>
						<td>@l.Birthdate</td>
						<td>@l.aa</td>
						<td>@l.aaDate</td>
						<td>@l.Salary</td>
					</tr>

				}
			</table>
		</div>
	</div>

	<div class="row">
		<div class="col">
			<a class="btn btn-dark" asp-Action="ExportExcel">匯出0</a>
			<button class="btn btn-dark" OnClick="location.href= '@Url.Action("ExportExcel")' ">匯出</button>
			<button class="btn btn-dark" @Url.Action("ExportExcel")>匯出2</button>
			<button class="btn btn-dark" asp-Action="ExportExcel">匯出3</button>
		</div>
	</div>
</div>


